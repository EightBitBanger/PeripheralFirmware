<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="PeripheralDevice" />
		<Option pch_mode="2" />
		<Option compiler="avr-gcc" />
		<Option virtualFolders="root/" />
		<Build>
			<Target title="Release">
				<Option output="bin/AVR-KERNEL.elf" prefix_auto="1" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="obj/" />
				<Option type="5" />
				<Option compiler="avr-gcc" />
			</Target>
			<Target title="Upload">
				<Option type="4" />
				<Option compiler="avr-gcc" />
				<ExtraCommands>
					<Add after="avrdude.exe -V -p m164a -c usbtiny -U flash:w:bin\AVR-KERNEL.hex" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="MCU" value="atmega164a" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-mmcu=atmega644p" />
			<Add option="-fexpensive-optimizations" />
			<Add option="-flto" />
			<Add option="-Os" />
			<Add option="-Wall" />
			<Add option="-std=c11" />
			<Add option="-DF_CPU=20000000UL" />
			<Add directory="../AVR-KERNEL" />
		</Compiler>
		<Linker>
			<Add option="-flto" />
			<Add option="-s" />
			<Add option="-mmcu=atmega164a" />
			<Add option="-Wl,-Map=$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).map,--cref" />
		</Linker>
		<ExtraCommands>
			<Add after='cmd /c &quot;avr-objdump -h -S $(TARGET_OUTPUT_FILE) &gt; $(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).lss&quot;' />
			<Add after="avr-objcopy -R .eeprom -R .fuse -R .lock -R .signature -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).hex" />
			<Add after="avr-objcopy --no-change-warnings -j .eeprom --change-section-lma .eeprom=0 -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).eep" />
		</ExtraCommands>
		<Unit filename="source/interrupt.h">
			<Option virtualFolder="root/" />
			<Option target="Release" />
		</Unit>
		<Unit filename="source/main.c">
			<Option compilerVar="CC" />
			<Option virtualFolder="root/" />
			<Option target="Release" />
		</Unit>
		<Unit filename="source/main.h">
			<Option virtualFolder="root/" />
			<Option target="Release" />
		</Unit>
		<Unit filename="source/network.h">
			<Option virtualFolder="root/" />
			<Option target="Release" />
		</Unit>
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
